{
  "manifest_version": 2,

  "name": "To do task",
  "description": "This extension allows to add tasks and set reminder",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "To Do List!"
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
